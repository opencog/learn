(define (break env)
	(display "yo duude> ")
	(let ((input (read)))
		(unless (or (eq? '. input) (eof-object? input))
			(catch #t (lambda () (local-eval input env))
				(lambda (key . args)
					(format #t "Oh no, Mr. Bill: ~A ~A\n" key args)
					*unspecified*))
			(break env))))

